# Development Secrets (sensitive - DO NOT COMMIT)
# Copy this to .env.secrets.dev and add your actual keys

# Django Secret Key (generate a new one for each environment)
SECRET_KEY=your-development-secret-key-here-make-it-long-and-random

# API Keys (use development/test keys when possible)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# Database credentials (if using external DB for dev)
DATABASE_PASSWORD=your-dev-database-password

# Email credentials (for testing email functionality)
EMAIL_HOST_USER=your-email@gmail.com
EMAIL_HOST_PASSWORD=your-app-password

# Third-party service keys (development/test keys)
STRIPE_SECRET_KEY=sk_test_your_stripe_test_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_key

# Social auth keys (if using social login)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Analytics keys (use test/dev properties)
GOOGLE_ANALYTICS_ID=your-dev-analytics-id

# Storage credentials (if using cloud storage)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_STORAGE_BUCKET_NAME=your-dev-bucket

# Redis credentials (if using external Redis)
REDIS_URL=redis://localhost:6379/0

# Monitoring/logging keys
SENTRY_DSN=your-sentry-dev-dsn